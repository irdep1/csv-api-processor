{
  "endpoint": "https://httpbin.org/post",
  "method": "POST",
  "payloadTemplate": {
    "customer": {
      "id": "$customerId",
      "name": "$customerName",
      "email": "$email"
    },
    "items": [
      {
        "productId": "$productId",
        "quantity": "$quantity"
      }
    ],
    "metadata": {
      "source": "CSV Import - Primary"
    }
  },
  "extractFromResponse": {
    "field": "responseData",
    "jsonPath": "json"
  }
}